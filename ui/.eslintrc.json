{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react-hooks"
  ],
  "env": {
    "browser": true,
    "es2023": true
  },
  "rules": {},
  "ignorePatterns": [
    "dist",
    "node_modules"
  ]
}
